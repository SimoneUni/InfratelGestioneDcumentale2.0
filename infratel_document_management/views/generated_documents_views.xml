        <!-- form view da copiare -->
        <record id="generated_documents_form_view" model="ir.ui.view">
            <field name="name">generated.documents.form.view</field>
            <field name="model">generated.documents</field>
            <field name="arch" type="xml">
                <form string="Generated Documents form"> 
                    <button class="oe_stat_button" icon="fa-folder-open" type="object" name="action_view_partner_all_documents">
                        <field string="Documenti appartenenti" name="documents_count" widget="statinfo"/>
                    </button> 
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                        </group>
                        <notebook>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

